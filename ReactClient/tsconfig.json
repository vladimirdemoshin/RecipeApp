{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node", 
    "baseUrl": "./src", 
    "paths": {
      "*": ["*", "src/*"] 
    },
  },
  "include": ["src"]
}